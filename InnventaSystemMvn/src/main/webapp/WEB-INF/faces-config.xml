<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Innventa</base-name>
            <var>innventa</var>
        </resource-bundle>
       
        <navigation-handler>org.primefaces.mobile.application.MobileNavigationHandler</navigation-handler>
    </application>
    <managed-bean>
        <managed-bean-name>personasConverter</managed-bean-name>
        <managed-bean-class>com.co.hsg.innventa.converter.PersonasConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>aclRolesConverter</managed-bean-name>
        <managed-bean-class>com.co.hsg.innventa.converter.AclRolesConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>aclRolesAccionConverter</managed-bean-name>
        <managed-bean-class>com.co.hsg.innventa.converter.AclRolesAccionConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>cuentasConverter</managed-bean-name>
        <managed-bean-class>com.co.hsg.innventa.converter.CuentasConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>estadosConverter</managed-bean-name>
        <managed-bean-class>com.co.hsg.innventa.converter.EstadosConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pedidosConverter</managed-bean-name>
        <managed-bean-class>com.co.hsg.innventa.converter.PedidosConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>productosConverter</managed-bean-name>
        <managed-bean-class>com.co.hsg.innventa.converter.ProductosConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>parametrosConverter</managed-bean-name>
        <managed-bean-class>com.co.hsg.innventa.converter.ParametrosConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>categoriasConverter</managed-bean-name>
        <managed-bean-class>com.co.hsg.innventa.converter.CategoriasConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>tercerosConverter</managed-bean-name>
        <managed-bean-class>com.co.hsg.innventa.converter.TercerosConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>unidadesMedidaConverter</managed-bean-name>
        <managed-bean-class>com.co.hsg.innventa.converter.UnidadesMedidaConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
     <managed-bean>
        <managed-bean-name>pedidosProductoConverter</managed-bean-name>
        <managed-bean-class>com.co.hsg.innventa.converter.PedidosProductoConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
     
</faces-config>
