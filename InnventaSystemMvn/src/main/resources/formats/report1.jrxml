	<queryString language="SQL">
		<![CDATA[SELECT
     pedidos.`cantidad_total` AS pedidos_cantidad_total,
     pedidos.`cantidad_pendientes` AS pedidos_cantidad_pendientes,
     pedidos.`fecha_pedido` AS pedidos_fecha_pedido,
     pedidos.`referencia` AS pedidos_referencia,
     pedidos.`estado` AS pedidos_estado,
     pedidos_producto.`CANTIDAD` AS pedidos_producto_CANTIDAD,
     pedidos_producto.`cantidad_entregada` AS pedidos_producto_cantidad_entregada,
     remisiones_producto.`cantidad` AS remisiones_producto_cantidad,
     remisiones.`referencia` AS remisiones_referencia,
     remisiones.`ESTADO` AS remisiones_ESTADO,
     remisiones.`fecha_remision` AS remisiones_fecha_remision,
     remisiones.`total_productos` AS remisiones_total_productos,
     productos.`nombre` AS productos_nombre,
     cuentas.`nombre` AS cuentas_nombre,
     pedidos_producto.`id_producto` AS pedidos_producto_id_producto,
     pedidos_producto.`id_pedido` AS pedidos_producto_id_pedido
FROM
     `pedidos` pedidos INNER JOIN `pedidos_producto` pedidos_producto ON pedidos.`id` = pedidos_producto.`id_pedido`
     INNER JOIN `remisiones_producto` remisiones_producto ON pedidos.`id` = remisiones_producto.`id_pedido`
     INNER JOIN `cuentas` cuentas ON pedidos.`id_cliente` = cuentas.`id`
     INNER JOIN `remisiones` remisiones ON remisiones_producto.`id_remision` = remisiones.`id`
     INNER JOIN `productos` productos ON remisiones_producto.`id_producto` = productos.`id`
     AND productos.`id` = pedidos_producto.`id_producto`]]>
	</queryString>
