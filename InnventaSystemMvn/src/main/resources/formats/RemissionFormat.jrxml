	<queryString language="SQL">
		<![CDATA[SELECT
     remisiones.`detalles` AS remisiones_detalles,
     remisiones.`entregado_a` AS remisiones_entregado_a,
     remisiones.`ESTADO` AS remisiones_ESTADO,
     remisiones.`fecha_remision` AS remisiones_fecha_remision,
     remisiones.`total_productos` AS remisiones_total_productos,
     remisiones_producto.`cantidad` AS remisiones_producto_cantidad,
     estados.`ESTADO` AS estados_ESTADO,
     cuentas.`nombre` AS cuentas_nombre,
     pedidos.`cantidad_pendientes` AS pedidos_cantidad_pendientes,
     pedidos.`cantidad_total` AS pedidos_cantidad_total,
     pedidos.`creado_por` AS pedidos_creado_por,
     pedidos.`eliminado` AS pedidos_eliminado,
     pedidos.`fecha_creacion` AS pedidos_fecha_creacion,
     pedidos.`fecha_entrega` AS pedidos_fecha_entrega,
     pedidos.`fecha_modificacion` AS pedidos_fecha_modificacion,
     pedidos.`fecha_pedido` AS pedidos_fecha_pedido,
     pedidos.`modificado_por` AS pedidos_modificado_por,
     pedidos.`observaciones` AS pedidos_observaciones,
     pedidos.`referencia` AS pedidos_referencia,
     pedidos.`estado` AS pedidos_estado,
     pedidos.`descripcion` AS pedidos_descripcion,
     remisiones.`modificado_por` AS remisiones_modificado_por
FROM
     `remisiones` remisiones INNER JOIN `remisiones_producto` remisiones_producto ON remisiones.`id` = remisiones_producto.`id_remision`
     INNER JOIN `pedidos` pedidos ON remisiones.`id_pedido` = pedidos.`id`
     INNER JOIN `estados` estados ON pedidos.`estado` = estados.`ID`
     INNER JOIN `cuentas` cuentas ON pedidos.`id_cliente` = cuentas.`id`]]>
	</queryString>
